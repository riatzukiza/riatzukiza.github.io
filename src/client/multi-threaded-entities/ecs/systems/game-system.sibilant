(define Game-system Spawnable
  (data-types [])
  (cached data (map this.buffers (b i) (.from-buffers (get this.data-types i) ...b)))
  (init (entities))

  (def-generic *update (args buffers data data-types)
    (for-of! d data (.step d))
    (then (.update this args data) results
          (for-of! entities results.entities
                   (const eid (.aquire this.entities))
                   )
          )))
(export Game-system)
